<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
      <param-name>price</param-name>
      <param-value>78</param-value>
  </context-param>
  <servlet>
  	<servlet-name>DollarServlet</servlet-name>
  	<display-name>DollarServlet</display-name>
  	<description></description>
  	<servlet-class>com.skilllogic.dollarservlet.DollarServlet</servlet-class>
  	<!-- <init-param>
  	  <param-name>price</param-name>
  	  <param-value>80</param-value>
  	</init-param> -->
  </servlet>
  <servlet-mapping>
  	<servlet-name>DollarServlet</servlet-name>
  	<url-pattern>/dollarservlet</url-pattern>
  </servlet-mapping>
</web-app>
